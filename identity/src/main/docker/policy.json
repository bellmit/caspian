{
    "admin_required": "role:admin",
    "monitor_role": "role:monitor",
    "service_role": "role:service",

    "cloud_admin_v3": "rule:admin_required and domain_id:default",
    "cloud_admin_v2": "rule:admin_required and project_id:${PROJECT_ADMIN_ID}",
    "cloud_admin": "rule:cloud_admin_v3 or rule:cloud_admin_v2",

    "cloud_monitor_v3": "rule:monitor_role and domain_id:default",
    "cloud_monitor_v2": "rule:monitor_role and project_id:${PROJECT_ADMIN_ID}",
    "cloud_monitor": "rule:cloud_monitor_v3 or rule:cloud_monitor_v2",

    "cloud_service_v3": "rule:service_role and domain_id:default",
    "cloud_service_v2": "rule:admin_required and project_id:${PROJECT_SERVICE_ID}",
    "cloud_service": "rule:cloud_service_v3 or rule:cloud_service_v2",

    "service_or_admin": "rule:admin_required or rule:service_role",
    "owner" : "user_id:%(user_id)s or user_id:%(target.token.user_id)s",
    "admin_or_owner": "(rule:admin_required and domain_id:%(target.token.user.domain.id)s) or rule:owner",
    "admin_or_cloud_admin": "rule:admin_required or rule:cloud_admin",
    "admin_and_matching_domain_id": "rule:admin_required and domain_id:%(domain_id)s",
    "service_admin_or_owner": "rule:service_or_admin or rule:owner",
    "domain_monitor": "rule:monitor_role and domain_id:%(domain_id)s",
    "cloud_admin_or_monitor_or_service": "rule:cloud_admin or rule:cloud_monitor or rule:cloud_service",
    "cloud_or_domain_monitor": "rule:cloud_monitor or rule:domain_monitor",
    "domain_admin_or_monitor": "rule:admin_and_matching_domain_id or rule:domain_monitor",

    "default": "rule:admin_required",

    "identity:get_region": "",
    "identity:list_regions": "",
    "identity:create_region": "rule:cloud_admin",
    "identity:update_region": "rule:cloud_admin",
    "identity:delete_region": "rule:cloud_admin",

    "identity:get_service": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:list_services": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:create_service": "rule:cloud_admin",
    "identity:update_service": "rule:cloud_admin",
    "identity:delete_service": "rule:cloud_admin",

    "identity:get_endpoint": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:list_endpoints": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:create_endpoint": "rule:cloud_admin",
    "identity:update_endpoint": "rule:cloud_admin",
    "identity:delete_endpoint": "rule:cloud_admin",

    "neutrino_owner": "role:neutrino_accounts_owner and domain_id:default",
    "neutrino_domain": "True:%(target.domain.neutrino_reserved)s",
    "default_domain": "'default':%(target.domain.id)s",
    "identity:get_domain": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor",
    "identity:list_domains": "rule:cloud_admin_or_monitor_or_service",
    "identity:create_domain": "rule:cloud_admin",
    "identity:update_domain": "(rule:neutrino_owner or (rule:cloud_admin and not rule:neutrino_domain)) and not rule:default_domain",
    "identity:delete_domain": "(rule:neutrino_owner or (rule:cloud_admin and not rule:neutrino_domain)) and not rule:default_domain",

    "reserved_projects": "'${PROJECT_ADMIN_ID}':%(target.project.id)s or '${PROJECT_SERVICE_ID}':%(target.project.id)s",
    "admin_and_matching_target_project_domain_id": "rule:admin_required and domain_id:%(target.project.domain_id)s",
    "admin_and_matching_target_project_id": "rule:admin_required and project_id:%(target.project.id)s",
    "admin_or_monitor_and_matching_target_project_domain_id": "(rule:admin_required or rule:monitor_role) and domain_id:%(target.project.domain_id)s",
    "admin_and_matching_project_domain_id": "rule:admin_required and domain_id:%(project.domain_id)s",
    "identity:get_project": "rule:cloud_admin_or_monitor_or_service or rule:admin_or_monitor_and_matching_target_project_domain_id",
    "identity:list_projects": "rule:admin_required or rule:cloud_monitor or rule:cloud_service or rule:domain_monitor",
    "identity:list_user_projects": "rule:cloud_admin_or_monitor_or_service or rule:owner or rule:domain_admin_or_monitor",
    "identity:create_project": "rule:cloud_admin or rule:admin_and_matching_project_domain_id",
    "identity:update_project": "(rule:cloud_admin or rule:admin_and_matching_target_project_domain_id or rule:admin_and_matching_target_project_id) and not rule:reserved_projects",
    "identity:delete_project": "(rule:cloud_admin or rule:admin_and_matching_target_project_domain_id) and not rule:reserved_projects",

    "reserved_users":"'${CLOUD_ADMIN_ID}':%(target.user.id)s or '${CPSA_USER_ID}':%(target.user.id)s or '${CLOUD_SERVICE_ID}':%(target.user.id)s",
    "precreated_users":"'${CPSA_USER_ID}':%(target.user.id)s or '${CLOUD_SERVICE_ID}':%(target.user.id)s",
    "admin_and_cpsa_in_default_domain_or_admin_project":"('${CPSA_USER_ID}':%(target.user.id)s or '${CLOUD_ADMIN_ID}':%(target.user.id)s) and ('default':%(target.domain.id)s or '${PROJECT_ADMIN_ID}':%(target.project.id)s) and rule:reserved_roles",
    "csa_in_default_domain_and_service_project":"'${CLOUD_SERVICE_ID}':%(target.user.id)s and (('default':%(target.domain.id)s and ('service':%(target.role.name)s or 'monitor':%(target.role.name)s)) or ('${PROJECT_SERVICE_ID}':%(target.project.id)s and ('admin':%(target.role.name)s or 'monitor':%(target.role.name)s)))",
    "admin_and_matching_target_user_domain_id": "rule:admin_required and domain_id:%(target.user.domain_id)s",
    "admin_or_monitor_and_matching_target_user_domain_id": "(rule:admin_required or rule:monitor_role) and domain_id:%(target.user.domain_id)s",
    "admin_and_matching_user_domain_id": "rule:admin_required and domain_id:%(user.domain_id)s",
    "identity:get_user": "rule:cloud_admin_or_monitor_or_service or rule:admin_or_monitor_and_matching_target_user_domain_id",
    "identity:list_users": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor",
    "identity:create_user": "rule:cloud_admin or rule:admin_and_matching_user_domain_id",
    "identity:update_user": "(rule:cloud_admin or rule:admin_and_matching_target_user_domain_id) and not rule:reserved_users",
    "identity:delete_user": "(rule:cloud_admin or rule:admin_and_matching_target_user_domain_id) and not rule:reserved_users",

    "admin_and_matching_target_group_domain_id": "rule:admin_required and domain_id:%(target.group.domain_id)s",
    "admin_or_monitor_and_matching_target_group_domain_id": "(rule:admin_required or rule:monitor_role) and domain_id:%(target.group.domain_id)s",
    "admin_and_matching_group_domain_id": "rule:admin_required and domain_id:%(group.domain_id)s",
    "identity:get_group": "rule:cloud_admin_or_monitor_or_service or rule:admin_or_monitor_and_matching_target_group_domain_id",
    "identity:list_groups": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor",
    "identity:list_groups_for_user": "rule:cloud_admin_or_monitor_or_service or rule:owner or rule:domain_admin_or_monitor",
    "identity:create_group": "rule:cloud_admin or rule:admin_and_matching_group_domain_id",
    "identity:update_group": "rule:cloud_admin or rule:admin_and_matching_target_group_domain_id",
    "identity:delete_group": "rule:cloud_admin or rule:admin_and_matching_target_group_domain_id",
    "identity:list_users_in_group": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor",
    "identity:remove_user_from_group": "rule:cloud_admin or rule:admin_and_matching_target_group_domain_id",
    "identity:check_user_in_group": "rule:cloud_admin_or_monitor_or_service or rule:admin_or_monitor_and_matching_target_group_domain_id",
    "identity:add_user_to_group": "rule:cloud_admin or rule:admin_and_matching_target_group_domain_id",

    "identity:get_credential": "rule:admin_required",
    "identity:list_credentials": "rule:admin_required or user_id:%(user_id)s",
    "identity:create_credential": "rule:admin_required",
    "identity:update_credential": "rule:admin_required",
    "identity:delete_credential": "rule:admin_required",

    "identity:ec2_get_credential": "rule:admin_or_cloud_admin or (rule:owner and user_id:%(target.credential.user_id)s)",
    "identity:ec2_list_credentials": "rule:admin_or_cloud_admin or rule:owner",
    "identity:ec2_create_credential": "rule:admin_or_cloud_admin or rule:owner",
    "identity:ec2_delete_credential": "rule:admin_or_cloud_admin or (rule:owner and user_id:%(target.credential.user_id)s)",

    "openstack_admin_role": "'openstack_admin':%(target.role.name)s",
    "neutrino_accounts_role": "'neutrino_accounts_owner':%(target.role.name)s",
    "reserved_roles": "'service':%(target.role.name)s or 'monitor':%(target.role.name)s or 'admin':%(target.role.name)s or rule:openstack_admin_role or rule:neutrino_accounts_role",
    "identity:get_role": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:list_roles": "rule:admin_required or rule:cloud_admin_or_monitor_or_service",
    "identity:create_role": "rule:cloud_admin",
    "identity:update_role": "rule:cloud_admin and not rule:reserved_roles",
    "identity:delete_role": "rule:cloud_admin and not rule:reserved_roles",

    "domain_admin_for_grants": "rule:admin_required and (domain_id:%(domain_id)s or domain_id:%(target.project.domain_id)s)",
    "domain_admin_or_monitor_for_grants": "(rule:admin_required or rule:monitor_role) and (domain_id:%(domain_id)s or domain_id:%(target.project.domain_id)s)",
    "project_admin_for_grants": "rule:admin_required and project_id:%(project_id)s",
    "identity:check_grant": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor_for_grants or rule:project_admin_for_grants",
    "identity:list_grants": "rule:cloud_admin_or_monitor_or_service or rule:domain_admin_or_monitor_for_grants or rule:project_admin_for_grants",
    "identity:create_grant": "(rule:cloud_admin or (rule:domain_admin_for_grants and not rule:openstack_admin_role) or (rule:project_admin_for_grants and not rule:openstack_admin_role)) and not rule:neutrino_accounts_role",
    "identity:revoke_grant": "((rule:cloud_admin or (rule:domain_admin_for_grants and not rule:openstack_admin_role) or (rule:project_admin_for_grants and not rule:openstack_admin_role)) and not rule:admin_and_cpsa_in_default_domain_or_admin_project and not rule:csa_in_default_domain_and_service_project) and not rule:neutrino_accounts_role",

    "admin_or_monitor_on_domain_filter": "(rule:admin_required or rule:monitor_role) and domain_id:%(scope.domain.id)s",
    "admin_on_project_filter": "rule:admin_required and project_id:%(scope.project.id)s",
    "identity:list_role_assignments": "rule:admin_required or rule:monitor_role",

    "identity:get_policy": "rule:cloud_admin_or_monitor_or_service",
    "identity:list_policies": "rule:cloud_admin_or_monitor_or_service",
    "identity:create_policy": "rule:cloud_admin",
    "identity:update_policy": "rule:cloud_admin",
    "identity:delete_policy": "rule:cloud_admin",

    "identity:change_password": "rule:owner and not rule:precreated_users",
    "identity:check_token": "rule:admin_or_owner or rule:cloud_service",
    "identity:validate_token": "rule:service_admin_or_owner or rule:cloud_service",
    "identity:validate_token_head": "rule:service_or_admin or rule:cloud_service",
    "identity:revocation_list": "rule:service_or_admin or rule:cloud_service",
    "identity:revoke_token": "rule:admin_or_owner",

    "identity:create_trust": "user_id:%(trust.trustor_user_id)s",
    "identity:list_trusts": "",
    "identity:list_roles_for_trust": "",
    "identity:get_role_for_trust": "",
    "identity:delete_trust": "",

    "identity:create_consumer": "rule:admin_required",
    "identity:get_consumer": "rule:admin_required",
    "identity:list_consumers": "rule:admin_required",
    "identity:delete_consumer": "rule:admin_required",
    "identity:update_consumer": "rule:admin_required",

    "identity:authorize_request_token": "rule:admin_required",
    "identity:list_access_token_roles": "rule:admin_required",
    "identity:get_access_token_role": "rule:admin_required",
    "identity:list_access_tokens": "rule:admin_required",
    "identity:get_access_token": "rule:admin_required",
    "identity:delete_access_token": "rule:admin_required",

    "identity:list_projects_for_endpoint": "rule:admin_required",
    "identity:add_endpoint_to_project": "rule:admin_required",
    "identity:check_endpoint_in_project": "rule:admin_required",
    "identity:list_endpoints_for_project": "rule:admin_required",
    "identity:remove_endpoint_from_project": "rule:admin_required",

    "identity:create_endpoint_group": "rule:admin_required",
    "identity:list_endpoint_groups": "rule:admin_required",
    "identity:get_endpoint_group": "rule:admin_required",
    "identity:update_endpoint_group": "rule:admin_required",
    "identity:delete_endpoint_group": "rule:admin_required",
    "identity:list_projects_associated_with_endpoint_group": "rule:admin_required",
    "identity:list_endpoints_associated_with_endpoint_group": "rule:admin_required",
    "identity:get_endpoint_group_in_project": "rule:admin_required",
    "identity:list_endpoint_groups_for_project": "rule:admin_required",
    "identity:add_endpoint_group_to_project": "rule:admin_required",
    "identity:remove_endpoint_group_from_project": "rule:admin_required",

    "identity:create_identity_provider": "rule:cloud_admin",
    "identity:list_identity_providers": "rule:cloud_admin_or_monitor_or_service",
    "identity:get_identity_providers": "rule:cloud_admin_or_monitor_or_service",
    "identity:update_identity_provider": "rule:cloud_admin",
    "identity:delete_identity_provider": "rule:cloud_admin",

    "identity:create_protocol": "rule:cloud_admin",
    "identity:update_protocol": "rule:cloud_admin",
    "identity:get_protocol": "rule:cloud_admin_or_monitor_or_service",
    "identity:list_protocols": "rule:cloud_admin_or_monitor_or_service",
    "identity:delete_protocol": "rule:cloud_admin",

    "identity:create_mapping": "rule:cloud_admin",
    "identity:get_mapping": "rule:cloud_admin_or_monitor_or_service",
    "identity:list_mappings": "rule:cloud_admin_or_monitor_or_service",
    "identity:delete_mapping": "rule:cloud_admin",
    "identity:update_mapping": "rule:cloud_admin",

    "identity:create_service_provider": "rule:cloud_admin",
    "identity:list_service_providers": "rule:cloud_admin_or_monitor_or_service",
    "identity:get_service_provider": "rule:cloud_admin_or_monitor_or_service",
    "identity:update_service_provider": "rule:cloud_admin",
    "identity:delete_service_provider": "rule:cloud_admin",

    "identity:get_auth_catalog": "",
    "identity:get_auth_projects": "",
    "identity:get_auth_domains": "",

    "identity:list_projects_for_groups": "",
    "identity:list_domains_for_groups": "",

    "identity:list_revoke_events": "",

    "identity:create_policy_association_for_endpoint": "rule:cloud_admin",
    "identity:check_policy_association_for_endpoint": "rule:cloud_admin_or_monitor_or_service",
    "identity:delete_policy_association_for_endpoint": "rule:cloud_admin",
    "identity:create_policy_association_for_service": "rule:cloud_admin",
    "identity:check_policy_association_for_service": "rule:cloud_admin_or_monitor_or_service",
    "identity:delete_policy_association_for_service": "rule:cloud_admin",
    "identity:create_policy_association_for_region_and_service": "rule:cloud_admin",
    "identity:check_policy_association_for_region_and_service": "rule:cloud_admin_or_monitor_or_service",
    "identity:delete_policy_association_for_region_and_service": "rule:cloud_admin",
    "identity:get_policy_for_endpoint": "rule:cloud_admin_or_monitor_or_service",
    "identity:list_endpoints_for_policy": "rule:cloud_admin_or_monitor_or_service",

    "identity:create_domain_config": "rule:cloud_admin",
    "identity:get_domain_config": "rule:cloud_admin_or_monitor_or_service",
    "identity:update_domain_config": "rule:neutrino_owner or (rule:cloud_admin and not rule:neutrino_domain)",
    "identity:delete_domain_config": "rule:neutrino_owner or (rule:cloud_admin and not rule:neutrino_domain)"
}
